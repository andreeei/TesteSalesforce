<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validacao_Meta</fullName>
    <active>true</active>
    <errorConditionFormula>IF(Meta_de_Volume__c &lt;0 || Meta_de_Volume__c &gt;100, true, false)</errorConditionFormula>
    <errorDisplayField>Meta_de_Volume__c</errorDisplayField>
    <errorMessage>O número inserido deve ser um número entre 0 e 100.</errorMessage>
</ValidationRule>
